.section ".text.stub"
.p2align 4
.global main
main:
	bti c
	// call _lfi_thread_create(&_lfi_pause)
	adr x0, _lfi_pause
	bl _lfi_thread_create
	// load address of trampoline table and then exit
	adrp x0, trampotable
	add x0, x0, :lo12:trampotable
.global _lfi_pause
_lfi_pause:
	bti jc
	mov x8, #94
	svc #0
	brk #0
.p2align 4
.global _lfi_retfn
_lfi_retfn:
	// special pseudo-instruction for calling the return runtime call
	//lfi:rtcall_return
	brk #0
.section ".data.trampotable"
.global trampotable
trampotable:
{{- range $sym := .exported }}
	.quad {{$sym}}
{{- end }}
{{- range $sym := .exposed}}
	.quad {{$sym}}
{{- end}}
.section .note.GNU-stack,"",@progbits
